{
    "name": "labs64/laravel-boilerplate",
    "description": "CC Dashboard",
    "type": "project",
    "keywords": [
        "php",
        "framework",
        "laravel",
        "laravel8",
        "admin",
        "theme",
        "starter",
        "bootstrap",
        "docker",
        "nodejs"
    ],
    "homepage": "http://codecrew.us/",
    "license": "MIT",
    "authors": [
        {
            "name": "Code Crew",
            "email": "crew@codecrew.us",
            "homepage": "http://codecrew.us/"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "ext-json": "*",
        "albertcht/invisible-recaptcha": "^1.9",
        "arcanedev/log-viewer": "^8.1",
        "creativeorange/gravatar": "^1.0",
        "davejamesmiller/laravel-breadcrumbs": "^5.2",
        "fruitcake/laravel-cors": "^1.0",
        "kyslik/column-sortable": "^6.1",
        "labs64/netlicensingclient-php": "^2.4",
        "laravel/framework": "^8.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "laravelcollective/html": "^6.1.0",
        "league/csv": "^9.7"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "doctrine/dbal": "^2.9",
        "filp/whoops": "^2.3",
        "fzaninotto/faker": "^1.8",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^9.0",
        "spatie/phpunit-watcher": "^1.12"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Database\\traits\\": "database/traits/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
